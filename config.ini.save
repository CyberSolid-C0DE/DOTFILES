#####################################################################################
################                POLYBAR CONFIG CHEF          ########################
#####################################################################################

[colors]
background = #aa2F343F
background-alt =#9b9b9b
#Grey
foreground = #C5C8C6
foreground-alt = #C5C8C6
primary = #F0C674
secondary = #8ABEB7
alert = #A54242
disabled = #707880
warning = #ff2f03

red = #ff2f03
black = #000000
orange = #ff7800
grey = #C5C8C6
gelb = #F0C674
blue = #0036ff
lila = #661990
pink = #ff00f7
green = #3cb371

#####################################################################################
################                Chefs Polybar 1              ########################
#####################################################################################


[bar/ChefsBar]
width = 98%
offset-x = 1%
height = 24pt
radius = 6
pseudo-transparency = true

background = ${colors.background}
foreground = ${colors.foreground}

line-size = 3pt

border-size = 3pt

padding-left = 0
padding-right = 1

module-margin = 1

separator = |
separator-foreground = ${colors.disabled}

modules-left = xworkspaces xwindow
modules-center = weather
modules-right = player pulseaudio network powermenu date

font-0 = jetBrainsMono Nerd Font:size=11;1
font-1 = Aristotelica Icons:size=15;4
font-2 = "Material Icons":size=20;2
font-3 = NotoEmoji:style=Book:scale=10

cursor-click = pointer
cursor-scroll = ns-resize

enable-ipc = true
monitor = DP-0


#####################################################################################
################                Chefs STATUSBAR              ########################
#####################################################################################


[bar/bar2]
width = 98%
offset-x = 1%
height = 24pt
radius = 6
pseudo-transparency = true

background = ${colors.background}
foreground = ${colors.foreground}

line-size = 3pt

border-size = 3pt

padding-left = 0
padding-right = 1

module-margin = 1

separator = |
separator-foreground = ${colors.disabled}

modules-left = xworkspaces sysmenu
modules-center =
modules-right = root GamesBackUps BackUps cpu gpu memory gpu-cpu-temp date

font-0 = jetBrainsMono Nerd Font:size=11;1
font-1 = Aristotelica Icons:size=15;4
font-2 = "Material Icons":size=20;2
font-3 = NotoEmoji:style=Book:scale=8

cursor-click = pointer
cursor-scroll = ns-resize

enable-ipc = true


monitor = DP-2

#####################################################################################
################                      MODULE                 ########################
#####################################################################################

[settings]
screenchange-reload = true

[module/xworkspaces]
type = internal/xworkspaces

label-active = %index%
label-active-background = ${colors.background-alt}
label-active-underline= ${colors.lila}
label-active-padding = 1

label-occupied = %index%
label-occupied-padding = 1

label-urgent = %index%
label-urgent-background = ${colors.alert}
label-urgent-padding = 1

label-empty = %index%
label-empty-foreground = ${colors.disabled}
label-empty-padding = 1

[module/xwindow]
type = internal/xwindow
label = %title:0:60:...%


[module/pulseaudio]
type = internal/pulseaudio
format-volume = <label-volume> <bar-volume>
label-volume = "%{A1:systemsettings kcm_pulseaudio:}ï€¨  %percentage%%{A}"
use-ui-max = fales
format-volume-foreground = ${colors.primary}
label-muted = ï€¦
label-muted-foreground = ${colors.red}
bar-volume-width = 10
bar-volume-foreground-0 = #55aa55
bar-volume-foreground-1 = #55aa55
bar-volume-foreground-2 = #55aa55
bar-volume-foreground-3 = #55aa55
bar-volume-foreground-4 = #55aa55
bar-volume-foreground-5 = #f5a70a
bar-volume-foreground-6 = #ff5555
bar-volume-gradient = false
bar-volume-indicator = |
bar-volume-indicator-font = 2
bar-volume-fill = â”€
bar-volume-fill-font = 2
bar-volume-empty = â”€
bar-volume-empty-font = 2
bar-volume-empty-foreground = ${colors.foreground-alt}

[module/memory]
type = internal/memory
interval = 2
format-prefix = "%{A1:plasma-systemmonitor:}ï‡€ %{A}"
format-prefix-foreground = ${colors.primary}
label = %percentage_used:2%%

[module/cpu]
type = internal/cpu
interval = 2
format-prefix = "%{A1:plasma-systemmonitor:}ï‹› %{A}"
format-prefix-foreground = ${colors.primary}
label = %percentage:2%%

[module/date]
type = internal/date
interval = 1
date = %H:%M
label = %date%
label-foreground = ${colors.primary}

[module/powermenu]
type = custom/menu
expand-right = true
format-spacing = 1
label-open = "ï€‘"
label-open-foreground = ${colors.gelb}
label-close = "ï€‘"

label-close-foreground = ${colors.gelb}
label-separator = |
label-separator-foreground = ${colors.foreground}

;Powermenu
menu-0-0 = "Reboot "
menu-0-0-exec = menu-open-1
menu-0-0-foreground = ${colors.foreground}
menu-0-1 = "Power Off"
menu-0-1-exec = menu-open-2
menu-0-1-foreground = ${colors.foreground}
menu-0-2 = "Logout"
menu-0-2-exec = menu-open-3
menu-0-2-foreground = ${colors.foreground}

;Reboot
menu-1-0  = "Reboot "
menu-1-0-exec = systemctl reboot
menu-1-0-foreground = ${colors.red}
menu-1-1 = "Cancel"
menu-1-1-exec = menu-open-0
menu-1-1-foreground = ${colors.foreground}

;Shutdown
menu-2-0 = "Power off"
menu-2-0-exec = systemctl poweroff
menu-2-0-foreground = ${colors.red}
menu-2-1 = "Cancel "
menu-2-1-exec = menu-open-0
menu-2-1-foreground = ${colors.foreground}

;Logout
menu-3-0 = "Logout"
menu-3-0-exec = loginctl terminate-user chef
menu-3-0-foreground = ${colors.red}
menu-3-1 = "Cancel"
menu-3-1-exec = menu-open-0
menu-3-1-foreground = ${colors.foreground}

[module/gpu]
type = custom/script
exec = ~/polybar-scripts/system-nvidia-smi.sh
interval = 2
format-prefix-foreground = ${colors.primary}
format-prefix = "%{A1:nvidia-settings:}ï‰¬  %{A}"

[module/root]
type = internal/fs
mount-0 = /
interval = 10
fixed-values = true
spacing = 4
warn-percentage = 75
format-mounted-prefix-foreground =  ${colors.primary}
format-mounted-prefix = %{A1:dolphin /:}ğŸ“ Chef %{A}
label-mounted = %percentage_used%% of %total%
format-unmounted = <label-unmounted>
label-unmounted = %mountpoint%: not mounted
label-unmounted-foreground = #55

[module/GamesBackUps]
type = internal/fs
mount-0 = /mnt/GamesBackUps
interval = 10
fixed-values = true
spacing = 4
warn-percentage = 75
format-mounted-prefix-foreground =  ${colors.primary}
format-mounted-prefix = %{A1:dolphin /mnt/GamesBackUps/:}ğŸ® Games %{A}
label-mounted = %percentage_used%% of %total%
format-unmounted = <label-unmounted>
label-unmounted = %mountpoint%: not mounted
label-unmounted-foreground = #55

[module/BackUps]
type = internal/fs
mount-0 = /mnt/BackUps
interval = 10
fixed-values = true
spacing = 4
warn-percentage = 75
format-mounted-prefix-foreground =  ${colors.primary}
format-mounted-prefix = %{A1:dolphin /mnt/BackUps/:}ğŸ“¦ BackUp %{A}
label-mounted = %percentage_used%% of %total%
format-unmounted = <label-unmounted>
label-unmounted = %mountpoint%: not mounted
label-unmounted-foreground = #55

[module/network]
type = internal/network
interface = wlp5s0
interval = 1.0
format-connected = <label-connected>
label-connected = "%{A1:kcmshell5 networkmanagement:}ï‡« %{A}"
label-connected-background = ${colors.background}
label-connected-foreground = ${colors.primary}
format-disconnected = <label-disconnected>
label-disconnected = "%{A1:bash ~/Scripts/Wifi:}ï‰« %{A}"
label-disconnected-background = ${colors.background}
label-disconnected-foreground = ${colors.red}

[module/gpu-cpu-temp]
type = custom/script
exec = ~/polybar-scripts/gpu-temp.sh
;exec-if = pgrep -x openvpn
interval = 1.0
format-prefix = "ï‹‰ "
format-prefix-foreground = ${colors.primary}


[module/player]
type = custom/script
exec = ~/polybar-scripts/player-mpris-simple.sh
interval = 3
click-left = playerctl play-pause &
click-right = playerctl next &
click-middle = playerctl previous &
label = %output:0:50:...%
label-foreground = ${colors.primary}

[module/weather]
type = custom/script
exec = ~/polybar-scripts/weather-plugin.sh
tail = false
interval = 960
